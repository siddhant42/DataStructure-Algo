package dynamic2;

public class RodCut {
	public int find(int n) {
		if(n==1) return 1;
		int maxsum = 0;
		for(int i=1;i<=Math.sqrt(n)+1;i++) {
			int max = Math.max(i*find(n-i), i*(n-i));
			if(maxsum<max) maxsum  = max;
		}
		return maxsum;
	}
public static void main(String[] args) {
	int n = 8;
	int sum = new RodCut().find(n);
	System.out.println(sum);
}
}
