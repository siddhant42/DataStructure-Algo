package dynamic2;

public class CandiesSum {
	int find(String str,int len,int n,int sum) {
		if(len==n+1) return sum;
		for(int i=0;i<=n-len;i++) {
			sum+=Integer.parseInt(str.substring(i,i+len)); 
		}
		return find(str,len+1,n,sum);
	}
	public static void main(String[] args) {
		CandiesSum obj = new CandiesSum();
		//int[] s = {1,2,3};
		String str = "123";
		int n = obj.find(str,1,str.length(),0);
		System.out.println(n);

	}
}
