package dynamic2;

public class RodCut {
	public int find(int n) {
		if(n==1||n==0) return 0;
		int maxsum = 0;
		for(int i=1;i<=Math.sqrt(n)+1;i++) {
			int max = Math.max(i*find(n-i), i*(n-i));
			if(maxsum<max) maxsum  = max;
		}
		return maxsum;
	}
	int findMax(int n) {
		int[] s = new int[n+1];
		for(int i=2;i<=n;i++){
			s[i]=Math.max(s[i-1], Math.max(i*(n-i),(n-i)*s[i]))
		}
	}
	public static void main(String[] args) {
		int n = 8;
		int sum = new RodCut().find(n);
		System.out.println(sum);
	}
}
